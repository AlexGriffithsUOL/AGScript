program         ::= statement_list

statement_list  ::= { statement }

statement       ::= assignment ";"

assignment      ::= identifier "=" expression

expression      ::= term (("+" | "-") term)*

term            ::= factor (("*" | "/") factor)*

factor          ::= number | identifier | "(" expression ")"

identifier      ::= /[a-zA-Z_][a-zA-Z0-9_]*/
number          ::= /[0-9]+/
